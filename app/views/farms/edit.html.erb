<%= form_for @farm do |f|%>
    <%=f.label :name%>
    <%=f.text_field :name%> <br>

    <%=f.label :Type_of_Farm%>
    <%=f.text_field :farm_type%>

    <%=f.label :Acres%>
    <%=f.number_field :acres%>

    <br>
    Select Workers to Hire or Deselect Workers to Fire: <br>
    <%= f.collection_check_boxes :worker_ids, Worker.all, :id, :name  %>
    <br>


    <%=f.submit "Edit Farm"%>
<%end%>
